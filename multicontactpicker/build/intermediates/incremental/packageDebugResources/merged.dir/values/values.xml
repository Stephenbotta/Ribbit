<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="reference" name="mcpListPopupWindowStyleTheme"/>
    <attr format="reference" name="mcpToolbarTheme"/>
    <color name="azureColorAccent">#FFFFFF</color>
    <color name="azureColorPrimary">#009BFA</color>
    <color name="azureColorPrimaryDark">#008EF8</color>
    <color name="border_color">#F1F1F1</color>
    <color name="selection_bg_color">#9E9E9E</color>
    <string name="app_name">MultiContactPicker</string>
    <string name="picker_title">Select Contacts</string>
    <string name="tv_no_contacts">No contacts found</string>
    <string name="tv_select_all_btn_text">Select All</string>
    <string name="tv_select_btn_text_disabled">Finish</string>
    <string name="tv_select_btn_text_enabled">Finish(%1$s)</string>
    <string name="tv_unselect_all_btn_text">Un-select All</string>
    <style name="MultiContactPicker.Azure" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/azureColorPrimary</item>
        <item name="colorPrimaryDark">@color/azureColorPrimaryDark</item>
        <item name="mcpToolbarTheme">@style/Toolbar.Azure</item>
        <item name="mcpListPopupWindowStyleTheme">@style/Popup.Azure</item>
    </style>
    <style name="Popup.Azure" parent="Widget.AppCompat.Light.ListPopupWindow">

    </style>
    <style name="Toolbar.Azure" parent="ThemeOverlay.AppCompat.Dark.ActionBar">

    </style>
    <declare-styleable name="RoundedLetterView">
        <attr format="string" name="rlv_titleText"/>
        <attr format="dimension" name="rlv_titleSize"/>
        <attr format="color" name="rlv_titleColor"/>
        <attr format="color" name="rlv_backgroundColorValue"/>
    </declare-styleable>
</resources>